						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "ls_5g_data_enablment_az_kv",
							"type": "LinkedServiceReference"
						},
						"secretName": "secret-5g-data-enablement-dbricks-token",
						"secretVersion": ""
					},
					"newClusterNodeType": "Standard_DS5_v2",
					"newClusterNumOfWorker": "2",
